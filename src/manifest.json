{
	"name": "__MSG_extName__",
	"description": "__MSG_extDescription__",
	"version": "0.0.1",
	"manifest_version": 3,
	"default_locale": "en",
	"background": {
		"service_worker": "sw.bundle.js"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": [
				"https://identity.bitclout.com/*",
				"https://www.${NODE_HOSTNAME}/*",
				"https://${NODE_HOSTNAME}/*"
			],
			"js": [
				"index.bundle.js"
			]
		}
	],
	"host_permissions": [
		"https://${NODE_HOSTNAME}/*",
		"https://www.${NODE_HOSTNAME}/*",
		"https://${NODE_API_HOSTNAME}/*",
		"https://identity.bitclout.com/*"
	],
	"permissions": [
		"alarms",
		"scripting",
		"storage",
		"tabs"
	],
	"icons": {
		"16": "/icons/16.png",
		"32": "/icons/32.png",
		"48": "/icons/48.png",
		"128": "/icons/128.png"
	}
}
